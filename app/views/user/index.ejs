<!DOCTYPE html>
<html>
  <head>
    <title>MVVM</title>
  </head>
  <body>
    <h1>MVVM Example</h1>
    <% for (const user of users) { %>
    <div>
      <h2><%= user.name %></h2>
      <p>Email: <%= user.email %></p>
      <p>Password: <%= user.password %></p>
    </div>
    <% } %>
  </body>
</html>
